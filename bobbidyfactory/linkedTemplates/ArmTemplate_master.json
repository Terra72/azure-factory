{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "bobbidyfactory"
		},
		"FriezaDB_service_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'FriezaDB_service'"
		},
		"bulmadb_service_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'bulmadb_service'"
		},
		"namekstore_service_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'namekstore_service'"
		},
		"piccolows_bulmadb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'piccolows_bulmadb'"
		},
		"vegetaVM_server_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'vegetaVM_server'"
		},
		"namekstore_service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://namekstore.dfs.core.windows.net/"
		},
		"vegetaVM_server_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "D:\\nginx-1.24.0\\logs"
		},
		"vegetaVM_server_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "bobbidyfactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"FriezaDB_service_connectionString": {
						"value": "[parameters('FriezaDB_service_connectionString')]"
					},
					"bulmadb_service_connectionString": {
						"value": "[parameters('bulmadb_service_connectionString')]"
					},
					"namekstore_service_accountKey": {
						"value": "[parameters('namekstore_service_accountKey')]"
					},
					"piccolows_bulmadb_connectionString": {
						"value": "[parameters('piccolows_bulmadb_connectionString')]"
					},
					"namekstore_service_properties_typeProperties_url": {
						"value": "[parameters('namekstore_service_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "bobbidyfactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"vegetaVM_server_password": {
						"value": "[parameters('vegetaVM_server_password')]"
					},
					"vegetaVM_server_properties_typeProperties_host": {
						"value": "[parameters('vegetaVM_server_properties_typeProperties_host')]"
					},
					"vegetaVM_server_properties_typeProperties_userId": {
						"value": "[parameters('vegetaVM_server_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'bobbidyfactory_ArmTemplate_0')]"
			]
		}
	]
}